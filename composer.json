{
    "name": "railken/eloquent-mapper",
    "type": "library",
    "keywords": ["laravel", "eloquent-mapper"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/bag": "^1.2",
        "railken/laravel-eloquent-join": "0.1.*",
        "railken/cacheable": "0.1.*",
        "imanghafoori/eloquent-relativity": "^1.0",
        "ankurk91/laravel-eloquent-relationships": "^1.1",
        "railken/lara-eye": "^1.3",
        "ramsey/collection": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.*",
        "nilportugues/sql-query-formatter": "^1.2"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\EloquentMapper\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\EloquentMapper\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\EloquentMapper\\EloquentMapperServiceProvider"
            ]
        }
    }
}
